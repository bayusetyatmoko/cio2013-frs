<form action="/mhs/update/<%= mhs.id %>" method="POST" id="form-edit" class="form-bayuz">
	<h2 class="form-signin-heading">Edit Data Mahasiswa</h2>

	<div class="form-group">
	<label for="txtnrp">NRP</label>
	<input type="text" class="form-control" value="<%= mhs.nrp %>" name="nrp" id="txtnrp" maxlength=10 readonly=''>
	</div>

	<div class="form-group">
	<label for="txtnama">Nama</label>
	<input type="text" class="form-control" value="<%= mhs.nama %>" name="nama" id="txtnama" maxlength=45>
	</div>
	
	<input type="submit" class="btn btn-lg btn-primary btn-block" value="Perbarui"/>
</form>
